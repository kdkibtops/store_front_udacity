{
    "name": "storefront-backend-project",
    "version": "0.1.0",
    "description": "## Getting Started",
    "main": "server.ts",
    "scripts": {
        "start": "node dist/server.js",
        "prettier": "prettier --config .prettierrc src/**/*.ts --write",
        "lint": "eslint src/**/*.ts",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
        "test": "tsc&&db-migrate db:drop shopping_test&& set ENV=test&& db-migrate db:create shopping_test&& db-migrate --env test up && jasmine &&db-migrate db:drop shopping_test",
        "tsc": "tsc",
        "build": "npx tsc"
    },
    "author": "Mustafa Heidar",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.5.1",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/eslint": "^8.4.1",
        "@types/express": "^4.17.9",
        "@types/jasmine": "^3.10.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/pg": "^8.6.4",
        "@types/prettier": "^2.4.4",
        "@types/supertest": "^2.0.11",
        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^4.0.2",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts": "^0.3.0",
        "prettier": "^2.6.0",
        "supertest": "^6.2.2",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9"
    }
}